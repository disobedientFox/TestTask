<h2> All Nodes</h2>
<div class="row mt-5 mb-2">
    @{
        foreach (var node in Model.AllNodes)
        {
            <div class="col-lg-4">
                <h3> Node: @node.Title</h3>
            </div>
            <div class="spinner-border text-danger" role="status">
                <span class="sr-only">Loading...</span>
            </div>
        }
    }
</div>
